  	</profile>
  	<profile>
  		<id>java11</id>
  		<activation>
  			<jdk>11</jdk>
  		</activation>
  		<properties>
    		<maven.compiler.source>11</maven.compiler.source>
		    <maven.compiler.target>11</maven.compiler.target>
  		</properties>
  	</profile>
  </profiles>
</project>